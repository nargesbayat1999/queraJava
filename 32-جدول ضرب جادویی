public class Main {
    public static void main(String[] args) {
        for(int i = 1; i < 11; i++) {
            for(int j = 1; j < 11; j++)
                if(i==j)
                    System.out.print(0+ " ");
                else if (i>j)
                    System.out.print(i * j + " ");
                else if (i<j)
                    System.out.print((i * j )*-1 + " ");
            System.out.print("\n");
        }
    }
}
