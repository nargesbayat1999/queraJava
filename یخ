import java.util.Scanner;


public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int o = scanner.nextInt();

        if (o > 100)
            System.out.println("Steam");
        else if (o < 0)
            System.out.println("Ice");
        else
            System.out.println("Water");
    }
}
